// distance measurement with ulrasonic sensor
const int trigPin = 2; 
const int echoPin = 4;
void setup() 
{
Serial.begin(9600);
}
void loop()

{
pinMode(trigPin, OUTPUT);

digitalWrite(trigPin, LOW);

delayMicroseconds(2);

digitalWrite(trigPin, HIGH);

delayMicroseconds(10);

digitalWrite(trigPin, LOW);

pinMode(echoPin, INPUT);

}

